{
  "version": 2,
  "builds": [
    {
      "src": "vercel_build.sh",
      "use": "@vercel/static-build"
    },
    {
      "src": "manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "(.*)",
      "dest": "manage.py"
    }
  ]
}
